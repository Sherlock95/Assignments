#declare card_back = box
{
    <-3.155, -4.405, 0> 
    < 3.155,  4.405,  -0.015 >

    texture{
        pigment{
            image_map
            {
                png "img/cardback.png"
                map_type 0
                interpolate 2
                once
            } // image_map
            translate <-0.5, -0.5, 0> scale<6.31, 8.81, 1>
        }
        finish{ambient 0.25}
    }
}

